package textExcel;

public class EmptyCell implements Cell {
	private String text;
	public EmptyCell(){
		setText("");
		for(int i=0;i<10;i++){
			setText(getText() + " ");
		}
	}
	
	@Override
	public String abbreviatedCellText() {
		return(getText());
	}

	@Override
	public String fullCellText() {
		return this.text;
	}

	public String getText() {
		return this.text;
	}

	public void setText(String text) {
		for(int i=0;i<10;i++){
			this.text += (getText() + " ");
		}
	}

}
