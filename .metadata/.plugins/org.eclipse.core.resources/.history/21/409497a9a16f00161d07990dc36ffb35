
public class Calculate {
	public static int square(int number){
		return(number*number);
	}
	
	public static int cube(int number){
		return(number*number*number);
	}
	
	public static double average(double number, double number2){
		return((number+number2)/2);
	}
	
	public static double average(double number, double number2, double 3){
		return((number*number2*number3)/3);
	}
	
	public static double toDegrees(double angleMeasure){
		return(angleMeasure);
	}
	
	public static double toRadians(double angleMeasure){
		return(angleMeasure);
	}
	

}